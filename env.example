# Database
DATABASE_URL=postgresql://username:password@host/database_name

# AI
ANTHROPIC_API_KEY=your_anthropic_api_key

# Feature Flags
NEXT_PUBLIC_USE_CLAUDE_CODE=false  # Set to true to enable Claude Code for premium website generation

# Freestyle API
FREESTYLE_API_KEY=your_freestyle_api_key
FREESTYLE_API_URL=https://api.freestyle.sh
FREESTYLE_FIRECRACKER=false

# Stack Auth - Get from https://app.stack-auth.com/
STACK_PROJECT_ID=your_stack_project_id
STACK_PUBLISHABLE_CLIENT_KEY=your_stack_publishable_client_key
STACK_SECRET_SERVER_KEY=your_stack_secret_server_key

# Public Stack Auth keys (same as above but with NEXT_PUBLIC_ prefix)
NEXT_PUBLIC_STACK_PROJECT_ID=your_stack_project_id
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=your_stack_publishable_client_key 